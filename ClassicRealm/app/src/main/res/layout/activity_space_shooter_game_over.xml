<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SpaceShooter.SpaceShooterGameOver">

    <Button
        android:id="@+id/ssBackToMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="14dp"
        android:text="Back"
        app:layout_constraintBaseline_toBaselineOf="@+id/restartSpaceShooter"
        app:layout_constraintEnd_toStartOf="@+id/restartSpaceShooter"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/restartSpaceShooter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="143dp"
        android:text="Restart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ssBackToMenu" />
</androidx.constraintlayout.widget.ConstraintLayout>